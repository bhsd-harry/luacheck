{
	"name": "luacheck-browserify",
	"version": "0.0.0",
	"description": "Luacheck in browser",
	"keywords": [
		"luacheck",
		"lint",
		"browser"
	],
	"homepage": "https://github.com/bhsd-harry/luacheck#readme",
	"bugs": {
		"url": "https://github.com/bhsd-harry/luacheck/issues"
	},
	"license": "MIT",
	"files": [
		"src/dist.js"
	],
	"browser": "src/dist.js",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/bhsd-harry/luacheck.git"
	},
	"scripts": {
		"lint": "eslint src/*.js",
		"build": "cd src; node ./bundle.js && cd .. && esbuild src/wasm.js --charset=utf8 --bundle --minify --target=es2019 --format=iife --outfile=dist/index.min.js --external:url --external:module",
		"server": "npm run test:end; http-server .. -c-1 --cors &",
		"test:end": "pkill -x http-server"
	},
	"devDependencies": {
		"@bhsd/common": "^0.4.5",
		"@stylistic/eslint-plugin": "^2.3.0",
		"@stylistic/stylelint-plugin": "^2.0.0",
		"@typescript-eslint/eslint-plugin": "^7.15.0",
		"@typescript-eslint/parser": "^7.15.0",
		"esbuild": "^0.21.4",
		"eslint": "^8.56.0",
		"eslint-plugin-es-x": "^8.0.0",
		"eslint-plugin-eslint-comments": "^3.2.0",
		"eslint-plugin-jsdoc": "^48.5.2",
		"eslint-plugin-json-es": "^1.6.0",
		"eslint-plugin-markdown": "^4.0.1",
		"eslint-plugin-n": "^17.9.0",
		"eslint-plugin-promise": "^6.2.0",
		"eslint-plugin-regexp": "^2.6.0",
		"eslint-plugin-unicorn": "^54.0.0",
		"http-server": "^14.1.0",
		"luabundle": "^1.7.0",
		"stylelint": "^16.6.1",
		"stylelint-config-recommended": "^14.0.0",
		"typescript": "^5.5.3",
		"wasmoon": "^1.16.0"
	}
}
